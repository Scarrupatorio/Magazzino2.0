<%- include('bars/head', {title: 'Magazzino'}); %>
<%- include('bars/header'); %>
    <div class="w-bg-h w-p-7">
        <div class="w-f-w-b w-f-s-70 w-ta-c w-intro w-c-bisque">Magazzino</div>
        <div class="w-jc-c">
            <table class="w-p-t-4">
                <thead class="w-bg-beige w-m-t-4 w-f-s-20 w-f-w-b">
                    <tr>
                        <% for (var i = 0; i < title.length; i++) { %>
                            <th class="w-w-20"><%= title[i].nome %></th>
                        <% } %>
                    </tr>
                </thead>
                <tbody class="w-bg-beige">
                    <% for (var i = 0; i < item.length; i++) { %>
                        <tr>
                            <td><%= item[i].nome %></td>
                            <td><%= item[i].prezzo %></td>
                            <td><%= item[i].quantità %></td>
                            <td><%= item[i].codice %></td>
                            <td><%= item[i].disponibilità %></td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div> 
<%- include('bars/footer'); %>